{
  "name": "react-scaffold",
  "version": "0.1.0",
  "description": "A scaffold for building testable React apps using ES6.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capgemini/react-scaffold/issues"
  },
  "homepage": "https://github.com/capgemini/react-scaffold",
  "keywords": [
    "scaffold",
    "template",
    "react",
    "quickstart"
  ],
  "private": true,
  "engines": {
    "node": ">= 0.10",
    "npm": ">= 2.1"
  },
  "dependencies": {
    "babel": "6.0.0",
    "classnames": "2.1.2",
    "eventemitter3": "1.1.0",
    "express": "4.13.1",
    "fastclick": "1.0.6",
    "flux": "2.0.3",
    "lodash": "3.10.0",
    "material-ui": "0.10.1",
    "normalize.css": "3.0.3",
    "object-assign": "^3.0.0",
    "react": "0.13.3",
    "react-router": "0.13.3",
    "source-map-support": "0.3.2",
    "superagent": "1.2.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.0",
    "babel-core": "^5.8.3",
    "babel-eslint": "^3.1.26",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.8.0",
    "css-loader": "^0.14.5",
    "csscomb": "^3.1.7",
    "del": "^1.2.0",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "eslint-plugin-react": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-changed": "^1.2.1",
    "gulp-if": "^1.2.5",
    "gulp-load-plugins": "^0.10.0",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^1.2.1",
    "gulp-util": "^3.0.5",
    "jest-cli": "^0.4.18",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.2.0",
    "postcss": "^4.1.11",
    "postcss-loader": "^0.4.4",
    "psi": "^1.0.6",
    "run-sequence": "^1.1.0",
    "sass-loader": "^1.0.3",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1"
  },
  "jest": {
    "rootDir": "./src",
    "scriptPreprocessor": "../preprocessor.js",
    "unmockedModulePathPatterns": [
      "react"
    ]
  },
  "scripts": {
    "start": "node server.js",
    "lint": "eslint --ignore-path /dev/null -c ./.eslintrc.jest src gulpfile.babel.js webpack.config.js && csscomb src/components --lint",
    "comb": "csscomb src/components --verbose",
    "test": "eslint --ignore-path /dev/null -c ./.eslintrc.jest src && jest",
    "build": "gulp build",
    "serve": "gulp serve",
    "sync": "gulp sync"
  }
}
